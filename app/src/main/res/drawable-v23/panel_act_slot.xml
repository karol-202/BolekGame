<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		android:left="12dp"
		android:top="12dp"
		android:right="12dp"
		android:bottom="12dp">
		<color android:color="#60000000"/>
	</item>
	<item
		android:gravity="left"
		android:top="12dp"
		android:bottom="12dp">
		<shape>
			<size android:width="12dp"/>
			<gradient
				android:startColor="#A0000000"
				android:endColor="#60000000"/>
		</shape>
	</item>
	<item
		android:gravity="top"
		android:left="12dp"
		android:right="12dp">
		<shape>
			<size android:height="12dp"/>
			<gradient
				android:startColor="#60000000"
				android:endColor="#A0000000"
				android:angle="90"/>
		</shape>
	</item>
	<item
		android:gravity="right"
		android:top="12dp"
		android:bottom="12dp">
		<shape>
			<size android:width="12dp"/>
			<gradient
				android:startColor="#60000000"
				android:endColor="#A0000000"/>
		</shape>
	</item>
	<item
		android:gravity="bottom"
		android:left="12dp"
		android:right="12dp">
		<shape>
			<size android:height="12dp"/>
			<gradient
				android:startColor="#A0000000"
				android:endColor="#60000000"
				android:angle="90"/>
		</shape>
	</item>
	<item
		android:gravity="left|top">
		<shape>
			<size
				android:width="12dp"
				android:height="12dp"/>
			<gradient
				android:type="linear"
				android:startColor="#60000000"
				android:centerColor="#A0000000"
				android:endColor="#C0000000"
				android:angle="135"/>
		</shape>
	</item>
	<item
		android:gravity="top|right">
		<shape>
			<size
				android:width="12dp"
				android:height="12dp"/>
			<gradient
				android:type="linear"
				android:startColor="#60000000"
				android:centerColor="#A0000000"
				android:endColor="#C0000000"
				android:angle="45"/>
		</shape>
	</item>
	<item
		android:gravity="right|bottom">
		<shape>
			<size
				android:width="12dp"
				android:height="12dp"/>
			<gradient
				android:type="linear"
				android:startColor="#60000000"
				android:centerColor="#A0000000"
				android:endColor="#C0000000"
				android:angle="315"/>
		</shape>
	</item>
	<item
		android:gravity="left|bottom">
		<shape>
			<size
				android:width="12dp"
				android:height="12dp"/>
			<gradient
				android:type="linear"
				android:startColor="#60000000"
				android:centerColor="#A0000000"
				android:endColor="#C0000000"
				android:angle="225"/>
		</shape>
	</item>
</layer-list>